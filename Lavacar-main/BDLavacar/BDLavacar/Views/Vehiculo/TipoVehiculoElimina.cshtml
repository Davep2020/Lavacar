
@{
    ViewBag.Title = "TipoVehiculoElimina";
    Layout = "~/Views/Shared/_LayoutBDLavacar.cshtml";
}

<h2>Elimina Tipo Vehiculo</h2>

<form id="frmTipoVehiculoElimina" method="post">
    <div class="form-group">
        <label>Código de vehículo</label>
        <input type="text" name="codigo_TV" id="codigo_TV" class="form-control" value="@Model.codigo_TV" readonly />
    </div>

    <div class="form-group">
        <label>Tipo de vehículo</label>
        <input type="text" name="Tipo_TV" id="Tipo_TV" class="form-control" value="@Model.Tipo_TV" readonly />
    </div>

    <div class="form-group">
        <input type="submit" value="Eliminar" id="btnEliminar" class="btn btn-danger" />
    </div>

</form>